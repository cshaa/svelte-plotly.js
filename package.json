{
  "name": "svelte-plotly.js",
  "version": "0.2.1",
  "author": {
    "name": "Michal Gr≈ào (m93a)",
    "url": "https://github.com/m93a/"
  },
  "description": "Unoficial Plotly package for Svelte and SvelteKit",
  "homepage": "https://github.com/m93a/svelte-plotly.js",
  "bugs": {
    "url": "https://github.com/m93a/svelte-plotly.js/issues"
  },
  "packageManager": "yarn@1.22.5",
  "license": "MIT",
  "paths": {
    "*": [
      "src/types/*.d.ts"
    ]
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "sass": "^1.51.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "svelte2tsx": "^0.5.9",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module",
  "dependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/plotly.js": "^2.12.2",
    "lodash-es": "^4.17.21",
    "plotly.js-dist": "^2.12.1"
  },
  "types": "./Plot.svelte.d.ts",
  "exports": {
    ".": "./Plot.svelte"
  }
}
